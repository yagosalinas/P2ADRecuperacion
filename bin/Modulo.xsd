<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema " elementFormDefault="qualified" atributoFormDefault="no qualified">
	<xs:element name="modulos">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="modulo">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="nombre" type="xs:cadena"></xs:element>
                            <xs:element name="curso" type="xs:string"></xs:element>
                            <xs:element name="ciclo" tipo = "xs:int"></xs:element>
                            <xs:element name="horas" type="xs:int"></xs:element>
                        </xs:sequence>
                    </xs:complexType>
					<xs:atributte name="id" type="xs:int" default="0" use="required"></xs:atributte>
                </xs:element>
            </xs:sequence>
			<xs:atributte name="xmlns:xsi" type="xs:string"></xs:atributte>
			<xs:atributte name="xsi:noNamespaceSchemaLocation" type="xs:string"></xs:atributte>
		</xs:complexType>
	</xs:element>
 </xs:schema>
 
